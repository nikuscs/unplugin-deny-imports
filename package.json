{
  "name": "unplugin-deny-imports",
  "version": "0.1.5",
  "description": "Deny imports with full import trace support for Vite, Bun, Rollup, webpack, and esbuild",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./vite": {
      "import": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.js"
      },
      "require": {
        "types": "./dist/vite.d.cts",
        "default": "./dist/vite.cjs"
      }
    },
    "./bun": {
      "import": {
        "types": "./dist/bun.d.ts",
        "default": "./dist/bun.js"
      },
      "require": {
        "types": "./dist/bun.d.cts",
        "default": "./dist/bun.cjs"
      }
    },
    "./rollup": {
      "import": {
        "types": "./dist/rollup.d.ts",
        "default": "./dist/rollup.js"
      },
      "require": {
        "types": "./dist/rollup.d.cts",
        "default": "./dist/rollup.cjs"
      }
    },
    "./webpack": {
      "import": {
        "types": "./dist/webpack.d.ts",
        "default": "./dist/webpack.js"
      },
      "require": {
        "types": "./dist/webpack.d.cts",
        "default": "./dist/webpack.cjs"
      }
    },
    "./esbuild": {
      "import": {
        "types": "./dist/esbuild.d.ts",
        "default": "./dist/esbuild.js"
      },
      "require": {
        "types": "./dist/esbuild.d.cts",
        "default": "./dist/esbuild.cjs"
      }
    },
    "./presets": {
      "import": {
        "types": "./dist/presets.d.ts",
        "default": "./dist/presets.js"
      },
      "require": {
        "types": "./dist/presets.d.cts",
        "default": "./dist/presets.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint src/",
    "lint:fix": "oxlint src/ --fix",
    "check": "bun run typecheck && bun run lint",
    "test": "vitest run && bun test test/bun.test.ts",
    "test:watch": "vitest",
    "taze": "bunx taze",
    "release": "bun run check && npx bumpp && git push --follow-tags",
    "prepublishOnly": "bun run check && bun run build"
  },
  "keywords": [
    "unplugin",
    "vite",
    "bun",
    "rollup",
    "webpack",
    "esbuild",
    "deny-imports",
    "import-trace",
    "server-only",
    "client-only",
    "ssr",
    "tanstack-start",
    "tanstack-router",
    "remix",
    "react-router",
    "use-server",
    "use-client"
  ],
  "author": "Pedro Martins <hi@nikuscs.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikuscs/unplugin-deny-imports.git"
  },
  "bugs": {
    "url": "https://github.com/nikuscs/unplugin-deny-imports/issues"
  },
  "homepage": "https://github.com/nikuscs/unplugin-deny-imports#readme",
  "dependencies": {
    "micromatch": "^4.0.8",
    "unplugin": "^2.0.0"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.0.0",
    "bumpp": "^10.2.3",
    "oxlint": "^0.16.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.0"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  }
}
